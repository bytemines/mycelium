{
  "name": "@mycelish/cli",
  "version": "0.2.0",
  "description": "CLI for Mycelium - Universal AI Tool Orchestrator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mycelium": "./dist/index.js",
    "myc": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bytemines/mycelium"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@mycelish/core": "workspace:*",
    "better-sqlite3": "^12.6.2",
    "commander": "^14.0.3",
    "express": "^5.2.1",
    "jsonc-parser": "^3.3.1",
    "picocolors": "^1.1.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  }
}
