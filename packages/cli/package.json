{
  "name": "@mycelium/cli",
  "version": "0.1.0",
  "description": "CLI for Mycelium - Universal AI Tool Orchestrator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mycelium": "./dist/index.js",
    "myc": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "files": ["dist", "README.md"],
  "repository": {
    "type": "git",
    "url": "https://github.com/bytemines/mycelium"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@mycelium/core": "workspace:*",
    "commander": "^13.0.0",
    "express": "^5.2.1",
    "picocolors": "^1.1.1",
    "yaml": "^2.6.1"
  }
}
